{
  "name": "herculesteam-augeasproviders_base",
  "version": "2.0.1",
  "author": "Dominic Cleal, Raphael Pinson",
  "summary": "Augeas-based base providers for Puppet",
  "license": "Apache-2.0",
  "source": "https://github.com/hercules-team/augeasproviders_base",
  "project_page": "http://augeasproviders.com",
  "issues_url": "https://github.com/hercules-team/augeasproviders_base/issues",
  "description": "This module provides providers for Puppet base types using the Augeas configuration API library.",
  "dependencies": [
    {
      "name": "herculesteam/augeasproviders_core",
      "version_requirement": ">=2.0.0 < 3.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7",
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04",
        "16.04",
        "18.04",
        "18.10"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 2.7.0 < 5.0.0"
    }
  ]
}
